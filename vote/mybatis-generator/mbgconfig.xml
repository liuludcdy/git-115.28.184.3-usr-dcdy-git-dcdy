<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE generatorConfiguration  
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"  
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<classPathEntry
		location="mysql-connector-java-5.1.21.jar" />

	<context id="DB2Tables" targetRuntime="MyBatis3">
		<plugin type="org.mybatis.generator.plugins.MysqlPaginationPlugin" />
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<commentGenerator>
			<property name="suppressAllComments" value="false" />
		</commentGenerator>

		<!-- JDBC 连接信息 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://192.168.11.2:3306/vote" userId="root"
			password="123456">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.dcdy.vote.dao"
			targetProject="../vote-dal/src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.dcdy.vote.dao"
			targetProject="../vote-dal/src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER" targetPackage="com.dcdy.vote.dao" 
			targetProject="../vote-dal/src/main/java">
			<property name="enableSubPackages" value="true"/>
		</javaClientGenerator>
		
 		<table tableName="vt_category" domainObjectName="VtCategory" ><property name="useActualColumnNames" value="false" /><generatedKey column="id" sqlStatement="MySql" identity="true"/></table>
 		<table tableName="vt_company" domainObjectName="VtCompany" ><property name="useActualColumnNames" value="false" /><generatedKey column="id" sqlStatement="MySql" identity="true"/></table>
 		<table tableName="vt_vote_log" domainObjectName="VtVoteLog" ><property name="useActualColumnNames" value="false" /><generatedKey column="id" sqlStatement="MySql" identity="true"/></table>
 		<table tableName="vt_board" domainObjectName="VtBoard" ><property name="useActualColumnNames" value="false" /><generatedKey column="id" sqlStatement="MySql" identity="true"/></table>
	</context>
</generatorConfiguration>